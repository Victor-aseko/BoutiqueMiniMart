{
  "expo": {
    "name": "MiniBoutique",
    "slug": "mobile",
    "scheme": "boutiqueminimart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/boutique.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/boutique.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aseko.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/boutique.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aseko.mobile"
    },
    "web": {
      "favicon": "./assets/boutique.png"
    },
    "extra": {
      "eas": {
        "projectId": "9fde5af2-80d0-434f-bc40-ace3ca22c48f"
      }
    },
    "owner": "aseko",
    "plugins": [
      "expo-web-browser",
      "expo-secure-store"
    ]
  }
}
